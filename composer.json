{
    "name": "decodelabs/harvest",
    "description": "PSR-15 HTTP stack without the mess",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1",
        "decodelabs/archetype": "^0.2.16",
        "decodelabs/coercion": "^0.2.7",
        "decodelabs/compass": "^0.1.5",
        "decodelabs/deliverance": "^0.2.11",
        "decodelabs/exceptional": "^0.4.4",
        "decodelabs/glitch-support": "^0.4.4",
        "decodelabs/singularity": "^0.2.2",
        "decodelabs/slingshot": "^0.1.1",
        "decodelabs/veneer": "^0.10.19",

        "psr/container": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "decodelabs/atlas": "^0.11.0",
        "decodelabs/genesis": "^0.8.3",
        "decodelabs/glitch": "^0.18.9",
        "decodelabs/pandora": "^0.2.11",
        "decodelabs/sanctum": "^0.1.4",
        "decodelabs/phpstan-decodelabs": "^0.6.7"
    },
    "provide": {
        "php-http/message-factory-implementation": "1.0",
        "psr/http-message-implementation": "1.0",
        "psr/http-factory-implementation": "1.0"
    },
    "suggest": {
        "decodelabs/sanctum": "Content security policy support"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Harvest\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    }
}
