{
    "name": "decodelabs/harvest",
    "description": "PSR-15 HTTP stack without the mess",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3",
        "decodelabs/coercion": "^0.2.7",
        "decodelabs/collections": "^0.10",
        "decodelabs/compass": "^0.3",
        "decodelabs/deliverance": "^0.2.11",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/glitch-support": "^0.5",
        "decodelabs/singularity": "^0.3",
        "decodelabs/slingshot": "^0.1.12",
        "decodelabs/veneer": "^0.12.1",

        "psr/container": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "decodelabs/atlas": "^0.12",
        "decodelabs/genesis": "^0.10",
        "decodelabs/pandora": "^0.3",
        "decodelabs/sanctum": "^0.3",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "provide": {
        "php-http/message-factory-implementation": "1.0",
        "psr/http-message-implementation": "1.0",
        "psr/http-factory-implementation": "1.0"
    },
    "suggest": {
        "decodelabs/sanctum": "Content security policy support"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Harvest\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
