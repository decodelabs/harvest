{
    "name": "decodelabs/harvest",
    "description": "PSR-15 HTTP stack without the mess",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/collections": "^0.11.3",
        "decodelabs/compass": "^0.3.1",
        "decodelabs/deliverance": "^0.3.3",
        "decodelabs/enumerable": "^0.2.7",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/nuance": "^0.1.2",
        "decodelabs/singularity": "^0.3.4",
        "decodelabs/slingshot": "^0.2.1",
        "decodelabs/veneer": "^0.12.8",

        "psr/container": "^2.0.2",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0.2",
        "psr/http-server-middleware": "^1.0.2",

        "nesbot/carbon": "^3"
    },
    "require-dev": {
        "decodelabs/atlas": "^0.13.1",
        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/tagged": "^0.20.1"
    },
    "conflict": {
        "decodelabs/atlas": "<0.13",
        "decodelabs/tagged": "<0.20"
    },
    "provide": {
        "php-http/message-factory-implementation": "1.0",
        "psr/http-message-implementation": "1.0",
        "psr/http-factory-implementation": "1.0"
    },
    "suggest": {
        "decodelabs/sanctum": "Content security policy support"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Harvest\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
